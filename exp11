#include<stdio.h>
#include<stdlib.h>
#include<stdbool.h>

bool isSafe(int row, int col, int *board, int n){
	for(int i = 0; i<row;i++){
		int prevCol = board[i];
		if(prevCol == col){
			return false;
		}
		if(abs(prevCol - col)==abs(i-row))
			return false;
	}
	return true;
}
int solveNQueen(int row, int *board, int n){
	if(row==n){
		return 1;
	}
	int count = 0;
	for(int col = 0; col<n;col++){
		if(isSafe(row, col,board,n)){
			board[row] = col;
			count += solveNQueen(row+1,board,n);
		}
	}
	return count;
}
int nQueen(int n){
	int board[n];
	return solveNQueen(0,board,n);
}
int main(){
	int n;
	scanf("%d",&n);
	printf("%d\n",nQueen(n));
	return 0;
}
